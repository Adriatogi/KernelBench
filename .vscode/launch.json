    "version": "0.2.0",
    "configurations": [
        {
            "name": "Single Sample",
            "type": "debugpy",
            "request": "launch",
            "program": "scripts/generate_and_eval_single_sample.py",
            "args": [
                "dataset_src=huggingface",
                "level=2",
                "problem_id=40",
                "server_type=archon",
                "model_name=individual_gpt-4-turbo",
                "archon_config_path=archon_configs/gpt-4-turbo.json"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Generate Samples",
            "type": "debugpy",
            "request": "launch",
            "program": "scripts/generate_samples.py",
            "args": [
                "run_name=simple_fuse_level_1",
                "dataset_src=huggingface",
                "level=1",
                "num_workers=50",
                "server_type=archon",
                "model_name=gpt_4o_simple_fuse",
                "archon_config_path=archon_configs/gpt-4o-simple-fuse.json"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Evaluate Generations",
            "type": "debugpy",
            "request": "launch",
            "program": "scripts/eval_from_generations.py",
            "args": [
                "level=1",
                "run_name=simple_fuse_level_1",
                "dataset_src=local",
                "num_gpu_devices=8",
                "timeout=300"
            ],
            "console": "integratedTerminal"
        }
    ]
}